   "source": [
    "import numpy as np\n",
    "x=np.random.uniform(-10, 10)\n",
    "eeta=0.005\n",
    "epochs=200\n",
    "iteration=0\n",
    "while iteration<epochs:\n",
    "    slope=4*(x**3)+6*x\n",
    "    step=(-eeta)*slope\n",
    "    x=x+step\n",
    "    iteration=iteration+1\n",
    "x=round(x)\n",
    "obj = x**4+3*(x**2)+10\n",
    "print('x =',x)\n",
    "print('Global Minimum Point =',obj)"
   ]
  }
 ],
